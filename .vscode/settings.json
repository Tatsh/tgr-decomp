{
  "C_Cpp.default.compileCommands": "builddir/compile_commands.json",
  "[c]": {
    "files.eol": "\r\n"
  },
  "[cpp]": {
    "files.eol": "\r\n"
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "cmake.configureArgs": ["-G", "Ninja", "-DBUILD_LAUNCHER=ON"],
  "cmake.sourceDirectory": "${workspaceFolder}/boss-rally",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "editor.insertSpaces": true,
  "editor.rulers": [100],
  "editor.tabSize": 2,
  "editor.wordWrapColumn": 100,
  "esbonio.sphinx.confDir": "docs",
  "eslint.lintTask.enable": true,
  "files.associations": {
    "*.rh": "c"
  },
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "mesonbuild.configureOptions": [
    "--cross-file",
    "${workspaceFolder}/linux-i686-w64-mingw32.txt",
    "-Dwarning_level=3",
    "-Dwerror=true"
  ],
  "python.analysis.autoImportCompletions": true,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.diagnosticSeverityOverrides": {
    "reportUnknownArgumentType": "none",
    "reportUnknownMemberType": "none",
    "reportUnknownVariableType": "none"
  },
  "python.analysis.importFormat": "relative",
  "python.analysis.indexing": true,
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.pytestParameters": true,
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.packageIndexDepths": [
    {
      "depth": 100,
      "name": ""
    }
  ],
  "python.analysis.stubPath": ".stubs",
  "python.analysis.typeCheckingMode": "strict",
  "python.formatting.provider": "yapf",
  "python.languageServer": "Pylance",
  "python.linting.flake8Enabled": false,
  "python.linting.pylintEnabled": false,
  "yaml.format.printWidth": 100
}
